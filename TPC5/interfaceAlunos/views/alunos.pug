extends layout

block content
  .w3-card-4
    header.w3-container.w3-blue
      h1 
          = tit
          a.w3-button.w3-green.w3-round(href="/adicionar-aluno") + 
    .w3-container
      table.w3-table-all
        tr
          th Id
          th Nome
          th Git Link
        each a in alunos
          tr
            td
             a(href=`/alunos/${a._id}`)= a._id
            td= a.nome
            td 
             a(href=`${a.gitlink}`)= a.gitlink




    footer.w3-container.w3-blue
